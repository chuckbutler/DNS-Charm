;
; BIND data file, generated by DNS Charm for {{domain}}
;
$TTL    604800
;@      IN      RR  ADDR 
{%- for record in data['SOA'] %}
@	IN 	SOA	{{record['addr']}}	{{record['owner']}} (
							{{record['serial']}}
							{{record['refresh']}}
							{{record['update-retry']}}
							{{record['expiry']}}
							{{record['update-retry']}} )
{% endfor %}
{%- for record in data['NS'] %}
{{record['owner-name']}}	IN     NS	{{record['addr']}}
{%- endfor %}
{%- for record in data['A'] %}
{{record['alias']}}	IN     A	{{record['addr']}}
{%- endfor %}
{%- for record in data['CNAME'] %}
{{record['alias']}}     	IN      CNAME	{{record['addr']}}
{% endfor %}
{%- for record in data['NAPTR'] %}
{{record['alias']}}	IN 	NAPTR	{{record['order']}}	{{record['pref']}}	{{record['flag']}}	{{record['params']}}	{{record['regexp']}}	{{record['replace']}}
{% endfor %}
{%- for record in data['SRV'] %}
{{record['alias']}}	{{record['class']}}	SRV 	{{record['priority']}}	{{record['weight']}}	{{record['port']}}	{{record['target']}}
{% endfor %}